# Scraper Configuration
TEST_MODE=0
# 0 = Full scrape (all products from all brands)
# 1 = Quick test (1 product from 1 random brand)
# 2 = Standard test (2 products from each brand)

# Schedule Configuration
USE_SCHEDULE=true
# true = Distribute brands across days (3 brands per day)
# false = Run all brands at once

# Supabase Configuration
USE_SUPABASE=true
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Railway Cron (set this in Railway dashboard)
# RAILWAY_CRON_SCHEDULE=0 0 * * *
# This will run at midnight UTC daily
